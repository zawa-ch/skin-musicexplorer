					<link Source="Playlist" SourcePin="Count" TargetPin="Value2"/>
					<link Source="Playlist" SourcePin="Duration" TargetPin="Value3"/>
					<link Source="Playlist" SourcePin="Size" TargetPin="Value4"/>
				</property>
				<property name="Expression" value="%TextValue1 :: %TextValue2, %TextValue3, %TextValue4"/>
			</properties>
		</provider>
		<provider name="Skin_RotaryPlayingFileInfoTimer" class="LogicalOperatorTimer">
			<properties>
				<property name="Bindings">
					<link Source="Skin_RotaryPlayingFileInfoSelector" SourcePin="Position" TargetPin="Action" Expression="(Value + ScrollDelta) MOD 4"/>
				</property>
				<property name="Interval" value="4000"/>
			</properties>
		</provider>
		<provider name="Skin_RotaryPlayingFileInfoMultiplexer" class="LogicalOperatorMultiplexer">
			<properties>
				<property name="Bindings">
					<link Source="Skin_RotaryPlayingFileInfoSelector" SourcePin="Position" TargetPin="Switch" Expression="Value + 1"/>
					<link Source="prvPlayingFileInfo" SourcePin="FileInfo" TargetPin="Value1" TextFormat="%UserTemplate1"/>
					<link Source="prvPlayingFileInfo" SourcePin="FileInfo" TargetPin="Value2" TextFormat="Artist: %UserTemplate2"/>
					<link Source="prvPlayingFileInfo" SourcePin="FileInfo" TargetPin="Value3" TextFormat="Album: %UserTemplate3"/>
					<link Source="prvPlayingFileInfo" SourcePin="FileInfo" TargetPin="Value4" TextFormat="%UserTemplate5"/>
				</property>
			</properties>
		</provider>
		<provider name="Skin_RotaryPlayingFileInfoSelector" class="LogicalOperatorCounter">
			<properties>
				<property name="Module" value="4"/>
			</properties>
		</provider>
		<provider name="String_RotaryPlayingFileInfo" class="StringOperatorConcatenator">
			<properties>
				<property name="Bindings">
					<link Source="prvPlayerState" SourcePin="State" TargetPin="Value1" Expression=" NOT Value = 0"/>
					<link Source="Skin_RotaryPlayingFileInfoMultiplexer" SourcePin="Result" TargetPin="Value2"/>
				</property>
				<property name="Expression" value="%IF(%Value1,%TextValue2,)"/>
			</properties>
		</provider>
		<provider name="String_RotaryPlayingFileInfoWithNotification" class="StringOperatorConcatenatorNotificator">
			<properties>
				<property name="Bindings">
					<link Source="String_RotaryPlayingFileInfo" SourcePin="Result" TargetPin="Value1"/>
				</property>
				<property name="Expression" value="%TextValue1"/>
			</properties>
		</provider>
		<provider name="String_LibraryInfo" class="StringOperatorConcatenator">
			<properties>
				<property name="Bindings">
					<link Source="prvMusicLibraryManager" SourcePin="ActiveDataSource" TargetPin="Value1"/>
					<link Source="MLTable" SourcePin="Count" TargetPin="Value2"/>
					<link Source="MLTable" SourcePin="Duration" TargetPin="Value3"/>
					<link Source="MLTable" SourcePin="Size" TargetPin="Value4"/>
				</property>
				<property name="Expression" value="%TextValue1 :: %TextValue2, %TextValue3, %TextValue4"/>
			</properties>
		</provider>
		<provider name="MLF_WindowEdgeSelector" class="LogicalOperatorCommutator">
			<properties>
				<property name="Bindings">
					<link Source="MediaLibraryForm" SourcePin="Active" TargetPin="Value1"/>
					<link Source="Setting_PrimaryColorEdge" SourcePin="Position" TargetPin="Value2"/>
				</property>
				<property name="Expression" value="If(Value1, If(Value2, 2, 1), 0)"/>
			</properties>
		</provider>
		<provider name="DW_WindowEdgeSelector" class="LogicalOperatorCommutator">
			<properties>
				<property name="Bindings">
					<link Source="DropWheel" SourcePin="Active" TargetPin="Value1"/>
					<link Source="Setting_PrimaryColorEdge" SourcePin="Position" TargetPin="Value2"/>
				</property>
				<property name="Expression" value="If(Value1, If(Value2, 2, 1), 0)"/>
			</properties>
		</provider>
		<provider name="TC_FileInfo" class="StringOperatorConcatenatorNotificator">
			<properties>
				<property name="Bindings">
					<link Source="prvPlayingFileInfo" SourcePin="FileInfo" TargetPin="Value1" TextFormat="%IF(%UserTemplate2,%UserTemplate2 - ,)%UserTemplate1"/>
				</property>
			</properties>
		</provider>
		<provider name="QFI_WindowEdgeSelector" class="LogicalOperatorCommutator">
			<properties>
				<property name="Bindings">
					<link Source="QFI" SourcePin="Active" TargetPin="Value1"/>
					<link Source="Setting_PrimaryColorEdge" SourcePin="Position" TargetPin="Value2"/>
				</property>
				<property name="Expression" value="If(Value1, If(Value2, 2, 1), 0)"/>
			</properties>
		</provider>
	</providers>
	<localizations default="English.lng">
		<item>English.lng</item>
	</localizations>
</skin>
